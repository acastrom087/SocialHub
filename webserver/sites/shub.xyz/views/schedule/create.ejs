<%- include('../layouts/head.ejs') -%>


<section class="py-5">
    <div class="py-5 d-flex justify-content-center">
        <div class="w-25">
            <form action="save" method="post" class="needs-validation" novalidate>
                <h1 class="h3 mb-3 py-4 fw-normal text-center">Schedule</h1>
                <div class="form-floating py-1">
                    <input type="time" class="form-control" name="password" placeholder="Password" required
                        onChange={handleChange} />
                    <label>Time</label>
                </div>
                <div class="form-floating py-1">
                    <input type="date" class="form-control" name="birthday" placeholder="Birthday" required
                        onChange={handleChange} />
                    <label>Date </label>
                </div>
                <div class="py-3">
                    <button class="w-100 btn btn-lg btn-dark" type="submit" >Save</button>
                </div>
            </form>
        </div>
    </div>
</section>
<%- include('../layouts/footer.ejs') -%>